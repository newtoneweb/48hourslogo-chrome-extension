{
  "manifest_version": 3,
  "name": "48HoursLogo Downloader",
  "version": "0.1.0",
  "description": "Add Ð° button to Download entries from a 48HoursLogo contest page",
  "icons": {
    "16": "img/icons/16x16.png",
    "32": "img/icons/32x32.png",
    "48": "img/icons/48x48.png",
    "128": "img/icons/128x128.png"
